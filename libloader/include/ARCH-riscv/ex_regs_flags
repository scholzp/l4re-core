#pragma once

#include <l4/sys/types.h>
#include <l4/sys/thread.h>

namespace Ldr {

enum : l4_umword_t { Default_ex_regs_flags = 0 };

inline
bool validate_ex_regs_flags(l4_umword_t flags)
{
  return flags == 0;
}

}
